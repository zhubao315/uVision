{
  "name": "clawd-throttle-skill",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/liekzejaws/clawd-throttle"
  },
  "keywords": ["openclaw", "llm", "router", "cost-optimization", "mcp", "proxy"],
  "description": "OpenClaw skill that routes LLM requests to the cheapest capable model based on prompt complexity",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "start": "tsx src/index.ts",
    "dev": "tsx watch src/index.ts",
    "stats": "tsx src/cli.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "setup": "powershell -ExecutionPolicy Bypass -File scripts/setup.ps1",
    "setup:unix": "bash scripts/setup.sh"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.22.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "vitest": "^2.1.0"
  }
}
